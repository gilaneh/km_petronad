<?xml version="1.0" encoding="utf-8"?>

<odoo>
   <record id="feeds_list" model="ir.ui.view">
            <field name="name">km_petronad.feeds</field>
            <field name="model">km_petronad.feeds</field>
            <field name="arch" type="xml">
                <tree editable="top" multi_edit="1">
                    <header>
                        <button string="Feed Stock" name="feed_stock_btn" type="object"
                                class="btn btn-danger" groups="base.group_system"/>
                    </header>
                    <field name="feeds_date"/>
                    <field name="project"/>
                    <field name="feed_in"/>
                    <field name="feed_out"/>
                    <field name="feed_stock"/>
                    <field name="description"/>

                </tree>
            </field>
        </record>


    <record id="feeds_action" model="ir.actions.act_window">
        <field name="name">Feeds Data</field>
            <field name="res_model">km_petronad.feeds</field>
            <field name="view_mode">tree</field>
    </record>



    <menuitem name="feeds"
          sequence="21"
          id="feeds_menu"
          parent="settings_menu"
          action="feeds_action"/>


</odoo>