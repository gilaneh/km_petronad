<?xml version="1.0" encoding="utf-8"?>
<odoo>
       <record id="storage_tanks_list" model="ir.ui.view">
            <field name="name">km_petronad.storage_tanks</field>
            <field name="model">km_petronad.storage_tanks</field>
            <field name="arch" type="xml">
                <tree editable="bottom">
                    <field name="name"/>
                    <field name="location"/>
                    <field name="tank_date"/>
                    <field name="asset_id"/>
                    <field name="fluid"/>
                    <field name="capacity"/>
                    <field name="description"/>

                </tree>
            </field>
        </record>

    <record id="storage_tanks_action" model="ir.actions.act_window">
        <field name="name">tanks Data</field>
            <field name="res_model">km_petronad.storage_tanks</field>
            <field name="view_mode">tree,form</field>
    </record>

    <menuitem name="Tanks"
          sequence="21"
          id="storage_tanks_menu"
          parent="settings_menu"
          action="storage_tanks_action"/>

</odoo>